        android:id="@+id/hole14thL"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole14thL"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole15thL"
        app:layout_constraintEnd_toEndOf="@+id/hole15thL" />

    <ImageButton
        android:id="@+id/hole14thC"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole14thC"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole15thC"
        app:layout_constraintEnd_toStartOf="@+id/hole14thR"
        app:layout_constraintStart_toEndOf="@+id/hole14thL" />

    <ImageButton
        android:id="@+id/hole14thR"
        android:layout_width="124dp"
        android:layout_height="54dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole14thR"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole15thR"
        app:layout_constraintStart_toStartOf="@+id/hole15thR" />

    <ImageButton
        android:id="@+id/hole15thL"
        android:layout_width="185dp"
        android:layout_height="76dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole15thL"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/seventhMoleBtn"
        app:layout_constraintEnd_toEndOf="@+id/seventhMoleBtn" />

    <ImageButton
        android:id="@+id/hole15thC"
        android:layout_width="185dp"
        android:layout_height="76dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole15thC"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/eigthMoleBtn"
        app:layout_constraintEnd_toStartOf="@+id/hole15thR"
        app:layout_constraintStart_toEndOf="@+id/hole15thL" />

    <ImageButton
        android:id="@+id/hole15thR"
        android:layout_width="185dp"
        android:layout_height="75dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole15thR"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/ninethMoleBtn"
        app:layout_constraintStart_toStartOf="@+id/ninethMoleBtn" />

    <ImageButton
        android:id="@+id/seventhMoleBtn"
        android:layout_width="245dp"
        android:layout_height="108dp"
        android:layout_marginBottom="4dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleLeftLast"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        android:soundEffectsEnabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/eigthMoleBtn"
        android:layout_width="245dp"
        android:layout_height="108dp"
        android:layout_marginBottom="4dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLast"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        android:soundEffectsEnabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ninethMoleBtn"
        app:layout_constraintStart_toEndOf="@+id/seventhMoleBtn" />

    <ImageButton
        android:id="@+id/ninethMoleBtn"
        android:layout_width="245dp"
        android:layout_height="108dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleRightLast"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        android:soundEffectsEnabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.gusakov.library.PulseCountDown
        android:id="@+id/pulseCountDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elegantTextHeight="true"
        android:fontFamily="@font/bauhaus_93"
        android:paddingLeft="16dp"
        android:paddingTop="10dp"
        android:paddingRight="14dp"
        android:paddingBottom="8dp"
        android:text="3"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textDirection="firstStrong"
        android:textSize="100dp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:pc_endValue="0"
        app:pc_startValue="3" />

    <TextView
        android:id="@+id/startPauseMessageTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elegantTextHeight="true"
        android:fontFamily="@font/bauhaus_93"
        android:paddingLeft="16dp"
        android:paddingTop="10dp"
        android:paddingRight="14dp"
        android:paddingBottom="8dp"
        android:text="Pause"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textDirection="firstStrong"
        android:textSize="50dp"
        android:textStyle="normal"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

