        app:layout_constraintStart_toEndOf="@+id/hole11thL" />

    <ImageButton
        android:id="@+id/hole11thR"
        android:layout_width="62dp"
        android:layout_height="31dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole12thR"
        app:layout_constraintStart_toStartOf="@+id/hole12thR" />

    <ImageButton
        android:id="@+id/hole12thL"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole13thL"
        app:layout_constraintEnd_toEndOf="@+id/hole13thL" />

    <ImageButton
        android:id="@+id/hole12thC"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole12thC"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole13thC"
        app:layout_constraintEnd_toStartOf="@+id/hole12thR"
        app:layout_constraintStart_toEndOf="@+id/hole12thL" />

    <ImageButton
        android:id="@+id/hole12thR"
        android:layout_width="80dp"
        android:layout_height="44dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="hole12thR"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole13thR"
