        app:layout_constraintBottom_toTopOf="@+id/hole12thL"
        app:layout_constraintEnd_toEndOf="@+id/hole12thL" />

    <ImageButton
        android:id="@+id/hole11thC"
        android:layout_width="62dp"
        android:layout_height="31dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole12thC"
