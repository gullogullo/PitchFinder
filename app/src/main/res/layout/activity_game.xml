        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole11thC"
        app:layout_constraintEnd_toStartOf="@+id/hole10thR"
        app:layout_constraintStart_toEndOf="@+id/hole10thL" />

    <ImageButton
