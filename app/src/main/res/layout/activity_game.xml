<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_sky_background"
    app:layoutDescription="@xml/activity_game_scene"
    tools:context=".GameActivity">

    <ImageView
        android:id="@+id/errorView"
        android:layout_width="207dp"
        android:layout_height="166dp"
        android:background="@android:color/transparent"
        android:padding="10dp"
        android:src="@drawable/error_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scoreTxt"
        app:layout_constraintVertical_bias="0.183" />

    <TextView
        android:id="@+id/countDownTimerTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/yellow_button_regular"
        android:padding="10dp"
        android:scaleType="centerCrop"
        android:text="45.000"
        android:textColor="@color/main_orange"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/scoreTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/yellow_button_regular"
        android:padding="10dp"
        android:text="0"
        android:textColor="@color/main_orange"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/countDownTimerTxt" />

    <ImageButton
        android:id="@+id/pauseGameBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:background="@drawable/yellow_button_selector"
        android:padding="10dp"
        android:src="@drawable/pause_selector"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/hole9thL"
        android:layout_width="40dp"
        android:layout_height="26dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole10thL"
        app:layout_constraintEnd_toEndOf="@+id/hole10thL" />

    <ImageButton
        android:id="@+id/hole9thC"
        android:layout_width="40dp"
        android:layout_height="26dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole10thC"
        app:layout_constraintEnd_toStartOf="@+id/hole9thR"
        app:layout_constraintStart_toEndOf="@+id/hole9thL" />

    <ImageButton
        android:id="@+id/hole9thR"
        android:layout_width="41dp"
        android:layout_height="26dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
        android:scaleType="fitCenter"
        android:src="@drawable/hole_mole"
        app:layout_constraintBottom_toTopOf="@+id/hole10thR"
        app:layout_constraintStart_toStartOf="@+id/hole10thR" />

    <ImageButton
        android:id="@+id/hole10thL"
        android:layout_width="48dp"
        android:layout_height="30dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:contentDescription="HoleMiddleLeft"
        android:maxWidth="42dp"
        android:maxHeight="42dp"
